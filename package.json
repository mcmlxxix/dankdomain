{
  "name": "dankdomain",
  "version": "0.4.3",
  "description": "Dank Domain: the return of Hack & Slash (1990-2014)",
  "main": "ttymain",
  "publisher": "theflyingape",
  "engines": {
    "node": ">=6.11"
  },
  "scripts": {
    "build": "tsc -p ./src --outDir ./build",
    "term": "cd ./build; node ./node_modules/tty.js/bin/tty.js --config ./tty/files/tty.json",
    "test": "npm run build && ./build/logins.sh",
    "xterm": "cd ./build; cd -"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/theflyingape/dankdomain.git"
  },
  "keywords": [
    "classic",
    "game",
    "online",
    "terminal",
    "text"
  ],
  "author": "Robert Hurst",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/theflyingape/dankdomain/issues"
  },
  "homepage": "https://robert.hurst-ri.us/games/dankdomain",
  "dependencies": {
    "express": "^4.16.2",
    "fs-extra": "^2.1.2",
    "node-pty": "^0.6.10",
    "nodemailer": "^4.1.3",
    "nodemailer-smtp-transport": "^2.7.4",
    "sprintf-js": "^1.1.1",
    "sqlite-sync": "^0.3.6",
    "title-case": "^2.1.1",
    "tty.js": "^0.2.15",
    "ws": "^2.3.1",
    "xterm": "^2.9.2",
    "xvt": "^1.0.13"
  },
  "devDependencies": {
    "@types/express": "^4.0.37",
    "@types/fs-extra": "0.0.37",
    "@types/node": "0.0.2",
    "@types/sprintf-js": "0.0.27",
    "@types/ws": "0.0.38"
  }
}
