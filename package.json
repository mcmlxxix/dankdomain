{
  "name": "dankdomain",
  "version": "0.2.11",
  "description": "Dank Domain: the return of Hack & Slash (1990-2014)",
  "main": "ttymain",
  "publisher": "theflyingape",
  "engines": {
	  "vscode": "0.10.x"
  },
  "scripts": {
    "build": "tsc -p ./src --outDir ./build",
    "door": "tsc -p ./src/door --outDir ./build/door; cd ./build/door; node app; cd -",
    "start": "npm run build && node ./build/ttymain",
    "vscode:prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/theflyingape/dankdomain.git"
  },
  "keywords": [
    "classic",
    "game",
    "online",
    "terminal",
    "text"
  ],
  "author": "Robert Hurst",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/theflyingape/dankdomain/issues"
  },
  "homepage": "https://robert.hurst-ri.us/games/dankdomain",
  "dependencies": {
    "express": "^4.14.1",
    "fs-extra": "^2.0.0",
    "node-pty": "^0.6.2",
    "nodemailer": "^2.6.4",
    "sprintf-js": "^1.0.3",
    "sqlite-sync": "^0.3.6",
    "title-case": "^2.1.0",
    "tty.js": "^0.2.15",
    "ws": "^2.0.3",
    "xterm": "^2.3.2",
    "xvt": "^1.0.10"
  },
  "devDependencies": {
    "@types/express": "^4.0.35",
    "@types/fs-extra": "0.0.37",
    "@types/node": "0.0.2",
    "@types/nodemailer": "^1.3.32",
    "@types/sprintf-js": "0.0.27",
    "@types/ws": "0.0.38"
  }
}
